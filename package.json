{
  "name": "excel-stream-writer",
  "version": "1.0.0",
  "description": "Create xlsx in streaming mode in browser and nodejs",
  "main": "dist/xlsx-stream-writer.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:webpack": "webpack --config webpack.config.js",
    "start": "node dist/index.js",
    "start:express": "node --inspect -r ts-node/register examples/express/main.ts",
    "dev": "nodemon --exec ts-node src/xlsx-stream-writer.ts --watch src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --watch --config jest.config.js",
    "clinic-doctor": "clinic doctor --autocannon [ -c 1 -t 60 -a 2 -m GET http://localhost:3000/excel/download ] -- node -r ts-node/register examples/express/main.ts",
    "clinic-bubbleprof": "clinic bubbleprof --autocannon [ -c 1 -t 60 -a 2 -m GET http://localhost:3000/excel/download ] -- node -r ts-node/register examples/express/main.ts",
    "clinic-flame": "clinic flame --autocannon [ -c 1 -t 60 -a 2 -m GET http://localhost:3000/excel/download ] -- node -r ts-node/register examples/express/main.ts",
    "coverage": "ts-jest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Alfredo Benassi <alfrebenassi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "jszip": "^3.2.1",
    "stream-buffers": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ABenassi87/excel-stream-writter.git"
  },
  "keywords": [
    "excel",
    "xlsx",
    "stream",
    "write",
    "typescript"
  ],
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.15",
    "@types/jsforce": "^1.9.7",
    "@types/jszip": "^3.1.6",
    "@types/node": "^12.0.12",
    "@types/stream-buffers": "^3.0.3",
    "appmetrics-dash": "^5.0.0",
    "async": "^3.1.0",
    "autocannon": "^4.0.0",
    "axios": "^0.19.0",
    "clinic": "^4.1.0",
    "dotenv": "^8.0.0",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "jsforce": "^1.9.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "winston": "^3.2.1"
  }
}
